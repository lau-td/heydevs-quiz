{
  "kind": "collectionType",
  "collectionName": "quiz_questions",
  "info": {
    "singularName": "quiz-question",
    "pluralName": "quiz-questions",
    "displayName": "Quiz Question",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "score": {
      "type": "integer",
      "min": 0,
      "default": 0,
      "required": true
    },
    "content": {
      "type": "string",
      "required": true
    },
    "quiz": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::quiz.quiz",
      "inversedBy": "questions"
    },
    "answers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::quiz-question-answer.quiz-question-answer",
      "mappedBy": "question"
    }
  }
}
